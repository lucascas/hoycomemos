{
    "builds": [
      {
        "src": "api/*.php",
        "use": "@vercel/php"
      },
      {
        "src": "views/index.html",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/views/$1"
      }
    ]
  }
  